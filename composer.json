{
    "name": "foubian/krudbooster",
    "description": "KrudBooster is CRUD Generator for laravel.",
    "type": "library",
    "require": {
        "laravel/framework": ">=11",
        "php": ">=8.3",
        "barryvdh/laravel-dompdf": "^3.1",
        "maatwebsite/excel": "^3.1",
        "phpoffice/phpspreadsheet": "^1.29",
        "doctrine/dbal": "^4.2"
    },
    "license": "MIT",
    "keywords":[
        "laravel",
        "admin-panel",
        "backend",
        "crud-generator",
        "crudbooster",
        "crud",
        "generator"
    ],
    "autoload": {
        "psr-4": {
            "foubian\\krudbooster\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Foubian",
            "email": "fouad.elrharbi@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "foubian\\krudbooster\\CRUDBoosterServiceProvider"
            ]
        }
    }
}
